APP_NAME=TerpInsights
APP_ENV=local
APP_KEY=
APP_DEBUG=true
# Must match the URL users see (e.g. https://www.terpinsights.com for custom domain). No trailing slash.
APP_URL=http://localhost
# When using a custom domain behind a proxy (e.g. Render), set ASSET_URL to the same value as APP_URL so JS/CSS load from your domain (avoids CORS and broken hamburger/login).
# ASSET_URL=https://www.terpinsights.com
# Optional: default meta description for SEO (used in layouts)
# META_DESCRIPTION=Maryland cannabis market intelligence. Clear, monthly insights from official MCA data.
# Optional: Google Analytics measurement ID (leave empty to disable)
# GOOGLE_ANALYTICS_ID=G-LHKJ5L5WT0
# Optional: Google Tag Manager container ID (leave empty to disable)
# GTM_CONTAINER_ID=GTM-KPQGB2KW

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
# For custom domain (e.g. www.terpinsights.com): use .terpinsights.com so cookie works for www and non-www
SESSION_DOMAIN=null
# Set to true in production over HTTPS (required for login/session on Render and custom domain)
# SESSION_SECURE_COOKIE=true

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

# file = fast local cache (default). Use database only if you need shared cache across multiple instances.
CACHE_STORE=file
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
